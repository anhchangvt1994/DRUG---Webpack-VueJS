{
	"version": 2,
	"env": {
		"MAX_WORKERS": "2",
		"DURATION_TIMEOUT": "10000",
		"DISABLE_COMPRESS_HTML": "true",
		"POWER_LEVEL": "2",
		"BANDWIDTH_LEVEL": "1",
		"SERVER_LESS": "true",
		"PUPPETEER_SKIP_DOWNLOAD": "true",
		"DISABLE_DETECT_LOCALE": "true"
	},
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/node"
		},
		{
			"src": "server/index/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "server/dist/index.js"
		}
	]
}
